{
  "name": "@cofp/coffeeless-react-validators",
  "description": "An input validation package made with love by Coffeeless Programming",
  "version": "1.4.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean  && tsc --p tsconfig.base.json && tsc-alias",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:staged": "npm test --findRelatedTests",
    "test:watch": "npm test -- --watch --no-cache",
    "test:ci": "npm test -- --coverage",
    "clean": "rimraf dist/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coffeeless-Programming-Playground/coffeeless-react-validators.git"
  },
  "keywords": [
    "coffeeless",
    "react",
    "validation"
  ],
  "author": "Diego Salas Noain <dominicsc2hs@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Coffeeless-Programming-Playground/coffeeless-react-validators/issues"
  },
  "homepage": "https://github.com/Coffeeless-Programming-Playground/coffeeless-react-validators#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.10.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16"
  }
}